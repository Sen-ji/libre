					<a class="nav-item nav-link my-2 " href="/BD">Projet BD</a>
				</div>
				<div class="navbar-nav">
					{% if app.user %}
						<a class="nav-item nav-link" href="#">{{app.user.username}}</a>
						<a class="nav-item nav-link" href="/logout">logout</a>
					{% else %}
						<a class="nav-item nav-link" href="/login">Login</a>
						<a class="nav-item nav-link my-2 my-lg-0" href="/register">Register</a>
					{% endif %}

				</div>
			</div>
		</nav>
		{% block body %}{% endblock %}
		<footer class="text-muted bg-dark">
			<div class="container pt-5 my-3">
